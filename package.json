{
  "name": "@yincw/rework",
  "version": "0.0.1",
  "description": "A library of CSS utils",
  "main": "lib/index.js",
  "browser": "dist/rework.min.js",
  "module": "es/index.js",
  "typings": "lib/rework.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "lib",
    "es",
    "LICENSE",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development rollup -wc --bundleConfigAsCjs",
    "test": "jest --verbose",
    "test:coverage": "jest --coverage",
    "test:e2e": "cypress open",
    "prepublish": "npm run build && npm run typing:build && npm run typedoc:build && git add . && cz && standard-version",
    "build": "npm run clean && npm run build:es && npm run build:lib && npm run build:amd && npm run build:dist",
    "build:dist": "cross-env NODE_ENV=production rollup -c --bundleConfigAsCjs",
    "build:amd": "tsc --outDir dist --module amd",
    "build:lib": "tsc --outDir lib --module commonjs",
    "build:es": "tsc --outDir es --module es2015 --declaration true",
    "clean": "rimraf dist/* && rimraf lib/* && rimraf es/* && rimraf coverage/* && rimraf temp/*",
    "typing:dev": "api-extractor run --local --verbose",
    "typing:build": "api-extractor run",
    "typedoc:dev": "typedoc --watch",
    "typedoc:build": "typedoc",
    "docs": "api-documenter markdown --input-folder temp --output-folder docs",
    "docs:start": "docusaurus start",
    "docs:build": "docusaurus build",
    "docs:serve": "docusaurus serve",
    "docs:deploy": "set GIT_USER=yincw && docusaurus deploy",
    "docs:swizzle": "docusaurus swizzle",
    "docs:clear": "docusaurus clear",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "gen": "node scripts/generator.js --mode=gen",
    "del": "node scripts/generator.js --mode=del"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged & jest",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*./src/**/*.{ts,js,less,css,json,md}": [
      "prettier --write --ignore-unknown",
      "eslint 'src/**/*.{ts,js}'"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@babel/plugin-transform-runtime": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-typescript": "^7.24.6",
    "@babel/runtime-corejs3": "^7.24.6",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@docusaurus/module-type-aliases": "3.4.0",
    "@docusaurus/tsconfig": "3.4.0",
    "@docusaurus/types": "3.4.0",
    "@easyops-cn/docusaurus-search-local": "^0.44.3",
    "@microsoft/api-documenter": "^7.24.11",
    "@microsoft/api-extractor": "^7.44.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "autoprefixer": "^10.4.19",
    "babel-plugin-polyfill-corejs3": "^0.10.4",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "docusaurus-plugin-typedoc": "^1.0.3",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^4.2.5",
    "jest": "^29.7.0",
    "less": "^4.2.0",
    "lint-staged": "^10.2.13",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "postcss-preset-env": "^9.5.14",
    "prettier": "2.1.1",
    "rimraf": "^3.0.2",
    "rollup": "^4.18.0",
    "standard-version": "^9.0.0",
    "stylelint": "^16.6.1",
    "stylelint-config-standard": "^36.0.0",
    "ts-jest": "^29.1.3",
    "tslib": "^2.6.2",
    "typedoc": "^0.26.4",
    "typedoc-plugin-markdown": "^4.2.1",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@docusaurus/core": "3.4.0",
    "@docusaurus/preset-classic": "3.4.0",
    "@docusaurus/theme-live-codeblock": "^3.4.0",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "core-js": "^3.37.1",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rework": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0"
  },
  "keywords": [
    "rework",
    "Utils",
    "Tools",
    "TypeScript",
    "Babel",
    "Rollup"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yincw/rework.git"
  },
  "author": "yincw",
  "license": "MIT"
}
